{
  "expo": {
    "name": "ReactNativeAPI",
    "slug": "ReactNativeAPI",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.techdivine.nativeapi",
      "versionCode": 1,
      "permissions": [],
      "icon": "./assets/icon.png"
    },
    "description": "testing all native features on react native",
    "githubUrl": "https://github.com/tejasgawali4/RN_NativeAPI"
  }
}
